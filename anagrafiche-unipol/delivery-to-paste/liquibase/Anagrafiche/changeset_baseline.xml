<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">

	<changeSet id="create-1" author="pinnag" context="Anagrafiche_BANK_DIRECTORY_PLUS">
        <createTable tablespace="${tablesTablespaceName}" remarks="SWIFT Bank Directory Plus" tableName="BANK_DIRECTORY_PLUS">
            <column name="ID" type="number">
                <constraints primaryKey="true" primaryKeyName="BANKPLUS_PRIM"/>
            </column>
            <column name="MOD_FLAG" remarks="A = Addition / M = Modification / D = Deletion" type="varchar(1 BYTE)">
                <constraints nullable="false"/>
            </column>
            <column name="RECORD_KEY" remarks="The unique key of the record in the file." type="varchar(12 BYTE)">
                <constraints nullable="false"/>
            </column>
            <column name="OFFICE_TYPE" remarks="Indicates the status of the entity in the entities hierarchy. HO - Head Office / DB - Domestic Branch or department / SB - Sub Domestic Branch / FB - Foreign Branch / SF - Sub Foreign Branch / UC - Unclassified" type="varchar(2 BYTE)">
                <constraints nullable="false"/>
            </column>
            <column name="PARENT_OFFICE_KEY" remarks="Indicates the RECORD KEY of the closest entity upwards in the entities hierarchy" type="varchar(12 BYTE)">
                <constraints nullable="false"/>
            </column>
            <column name="HEAD_OFFICE_KEY" remarks="Indicates the RECORD KEY of the &quot;HO - Head Office&quot; in the entities hierarchy" type="varchar(12 BYTE)">
                <constraints nullable="false"/>
            </column>
            <column name="LEGAL_TYPE" remarks="Indicates the status of the entity in the legal hierarchy. L - Legal Entity / B - Business Entity / U - Unknown" type="varchar(1 BYTE)">
                <constraints nullable="false"/>
            </column>
            <column name="LEGAL_PARENT_KEY" remarks="Indicates the RECORD KEY of the &quot;L - Legal Entity&quot; in the legal hierarchy" type="varchar(12 BYTE)">
                <constraints nullable="false"/>
            </column>
            <column name="GROUP_TYPE" remarks="The type of entity that identifies the group." type="varchar(6 BYTE)"/>
            <column name="GROUP_PARENT_KEY" remarks="The record key of the parent entity. This value identifies the set of entities (records) belonging to the group" type="varchar(12 BYTE)"/>
            <column name="INSTITUTION_STATUS" remarks="Indicates the licence status of the institution" type="varchar(4 BYTE)"/>
            <column name="COOP_GROUP_KEY" remarks="This field indicates the Record_Key of the cooperative central bank for a cooperative bank group" type="varchar(12 BYTE)"/>
            <column name="ISO_LEI_CODE" remarks="ISO 17442 defines this Legal Entity Identifier" type="varchar(20 BYTE)"/>
            <column name="BIC8" remarks="BIC (party prefix, country, and party suffix)" type="varchar(8 BYTE)"/>
            <column name="BRANCH_BIC" remarks="The BIC branch identifier associated with the 8-character BIC" type="varchar(3 BYTE)"/>
            <column name="BIC" remarks="The unique BIC related to the institution from the BIC Directory" type="varchar(11 BYTE)"/>
            <column name="CHIPS_UID" remarks="The CHIPS Universal ID related to the institution" type="varchar(6 BYTE)"/>
            <column name="NATIONAL_ID" remarks="The National identifier of the institution/branch" type="varchar(15 BYTE)"/>
            <column name="CONNECTED_BIC" remarks="The connected BIC of the same institution, if available, or of its correspondent through which it connects" type="varchar(11 BYTE)"/>
            <column name="INSTITUTION_NAME" remarks="Institution trading name" type="varchar(105 BYTE)">
                <constraints nullable="false"/>
            </column>
            <column name="BRANCH_INFO" remarks="Branch information is derived from the BIC Directory" type="varchar(70 BYTE)"/>
            <column name="POB_NUMBER" remarks="Comma-separated field with the POB number of the institution/branch and the related ZIP code" type="varchar(35 BYTE)"/>
            <column name="STREET_ADDRESS_1" remarks="Street name" type="varchar(35 BYTE)"/>
            <column name="STREET_ADDRESS_2" remarks="Street number" type="varchar(35 BYTE)"/>
            <column name="STREET_ADDRESS_3" remarks="Building, building name, floor" type="varchar(35 BYTE)"/>
            <column name="STREET_ADDRESS_4" remarks="Area" type="varchar(35 BYTE)"/>
            <column name="CITY" remarks="City name of the institution/branch" type="varchar(35 BYTE)"/>
            <column name="CPS" remarks="County, province, state, or other administrative region of the owner institution/branc" type="varchar(90 BYTE)"/>
            <column name="ZIP_CODE" remarks="Zip code of the institution/branch" type="varchar(15 BYTE)"/>
            <column name="COUNTRY_NAME" remarks="The country name of the institution/branch as indicated in the ISO 3166" type="varchar(70 BYTE)">
                <constraints nullable="false"/>
            </column>
            <column name="ISO_COUNTRY_CODE" remarks="The ISO 3166-1 alpha-2 code of the country of the institution/branch" type="varchar(2 BYTE)">
                <constraints nullable="false"/>
            </column>
            <column name="TIMEZONE" remarks="This field will contain the business party suffix information" type="varchar(1 BYTE)"/>
            <column name="SUBTYPE_IND" remarks="The business type of the entity. Subtype indicator is provided only for records with a BIC." type="varchar(4 BYTE)"/>
            <column name="NET_CONNECTIVITY" remarks="Status of an entitys connection to SWIFT" type="varchar(3 BYTE)"/>
            <column name="BRANCH_QUALIFIERS" remarks="The operational type of the entity." type="varchar(35 BYTE)"/>
            <column name="SERVICE_CODES" remarks="SWIFT FIN service codes" type="varchar(60 BYTE)"/>
            <column name="SSI_GROUP_KEY" remarks="Indicates which SSI Group the entity belongs to in the SSI Plus files" type="varchar(12 BYTE)"/>
            <column name="IBAN_KEY" remarks="Identifies the RECORD KEY in the IBANPLUS txt file that contains the IBAN data for this entity." type="varchar(12 BYTE)"/>
            <column name="FIELD_A" remarks="Contains the future valid-from date of the whole record due to a change to its attributes" type="varchar(35 BYTE)"/>
            <column name="FIELD_B" remarks="National ID Type - National ID Type + /AN - National ID Type + /FN" type="varchar(70 BYTE)"/>
            <column name="REVISION" remarks="Revision" type="varchar(100 BYTE)">
                <constraints nullable="false"/>
            </column>
            <column defaultValueNumeric="0" name="VERSION" type="number">
                <constraints nullable="false"/>
            </column>
            <column defaultValueNumeric="0" name="PROPAGATE" type="number(1, 0)">
                <constraints nullable="false"/>
            </column>
            <column defaultValueNumeric="0" name="DELETED" type="number(1, 0)">
                <constraints nullable="false"/>
            </column>
            <column defaultValueComputed="CURRENT_TIMESTAMP" name="CREATED_TMST" remarks="Row creation Timestamp" type="timestamp(6)"/>
            <column defaultValueComputed="CURRENT_TIMESTAMP" name="UPDATED_TMST" remarks="Row edit Timestamp" type="timestamp(6)"/>
            <column name="REVISION_TMST" remarks="Revision creation Timestamp" type="timestamp(6)"/>
            <column defaultValueNumeric="1" name="ACTIVE_VERSION" remarks="Indicates if the record is an active version" type="number(1, 0)">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>
